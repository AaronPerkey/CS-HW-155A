package com.fmt;
/**
 * 
 * Models items company sells.
 * 
 * @author aaron, emma
 *
 */
public class Items extends InvoiceItems{

	private final String itemsQRCode;
	private final String typeOfSale;
	private final String name;

	public Items(String invoiceCode, String itemsQRCode, String cost, String rent, double quantity, String hoursBilled,
			String itemsQRCode2, String typeOfSale, String name) {
		super(invoiceCode, itemsQRCode, cost, rent, quantity, hoursBilled);
		itemsQRCode = itemsQRCode2;
		this.itemsQRCode = itemsQRCode;
		this.typeOfSale = typeOfSale;
		this.name = name;
	}

	public String getItemsQRCode() {
		return itemsQRCode;
	}

	public String getTypeOfSale() {
		return typeOfSale;
	}

	public String getName() {
		return name;
	}

	
	
}
